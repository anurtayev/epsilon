{
  "name": "epsilon",
  "version": "1.0.0",
  "description": "aspan back-end",
  "scripts": {
    "deploy": "scripts/deploy.sh",
    "install:sharp:linux": "SHARP_IGNORE_GLOBAL_LIBVIPS=1 npm install --arch=x64 --platform=linux sharp",
    "codegen:downloadSchema": "dotenv -- bash -c 'aws appsync get-introspection-schema --api-id $APPSYNC_API_ID --format SDL cloudformation/schema.graphql'",
    "codegen": "graphql-codegen --config codegen.yml",
    "s3:sync": "aws s3 sync s3://nurtai-pics-test s3://epsilon-development-media",
    "test": "jest",
    "start": "dotenv ts-node src/lambda/fs/index.ts"
  },
  "dependencies": {
    "@types/sharp": "^0.29.5",
    "aws-sdk": "^2.991.0",
    "dotenv-safe": "^8.2.0",
    "exifr": "^7.1.3",
    "graphql": "^16.2.0",
    "sharp": "^0.29.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-resolvers": "2.4.2",
    "@types/aws-lambda": "^8.10.89",
    "@types/jest": "^27.4.0",
    "@types/node": "^14.14.25",
    "dotenv-cli": "^4.1.1",
    "eslint": "^8.6.0",
    "jest": "^27.4.7",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
